{
  "name": "dashboard-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "migrations": "npx drizzle-kit generate",
    "migrate": "npx tsx src/scripts/run-migrations.ts",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "migrate-agents": "npx tsx src/scripts/migrate-agents-to-db.ts",
    "check-migration": "npx tsx src/scripts/check-migration.ts",
    "verify-agents": "npx tsx src/scripts/verify-migration-completeness.ts",
    "list-db-agents": "npx tsx src/scripts/check-db-agents.ts",
    "test-agents": "npx tsx src/scripts/test-agent-proposals.ts",
    "test-minimal": "npx tsx src/scripts/test-agent-proposals.ts --template=minimal",
    "test-flash": "npx tsx src/scripts/test-agent-proposals.ts --template=flash",
    "test-prime": "npx tsx src/scripts/test-agent-proposals.ts --template=prime",
    "update-agents-language": "npx tsx src/scripts/update-agents-language.ts",
    "create-minimal-agents": "npx tsx src/scripts/create-minimal-agents.ts",
    "check-expiring-proposals": "npx tsx src/scripts/check-expiring-proposals.ts",
    "test-minimal-quick": "npx tsx src/scripts/test-minimal-quick.ts",
    "test-minimal:designer": "npx tsx src/scripts/test-minimal-quick.ts --service=designer",
    "test-minimal:arquiteto": "npx tsx src/scripts/test-minimal-quick.ts --service=arquiteto",
    "test-minimal:photography": "npx tsx src/scripts/test-minimal-quick.ts --service=photography",
    "test-minimal:marketing": "npx tsx src/scripts/test-minimal-quick.ts --service=marketing-digital",
    "test-minimal:dev": "npx tsx src/scripts/test-minimal-quick.ts --service=desenvolvedor",
    "prepare": "husky",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "check-test-user": "npx tsx e2e/scripts/check-test-user.ts"
  },
  "dependencies": {
    "@ai-sdk/togetherai": "^1.0.1",
    "@clerk/localizations": "^3.13.11",
