    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@neondatabase/serverless": "^0.10.4",
    "@vercel/blob": "^1.1.1",
    "ai": "^5.0.2",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.41.0",
    "gsap": "^3.13.0",
    "lenis": "^1.3.8",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.482.0",
    "nanoid": "^4.0.2",
    "next": "^15.5.7",
    "posthog-js": "^1.281.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-easy-crop": "^5.5.3",
    "react-fast-marquee": "^1.6.5",
    "react-slick": "^0.30.3",
    "react-toastify": "^11.0.5",
    "slick-carousel": "^1.8.1",
    "stripe": "^18.4.0",
    "svix": "^1.74.1",
    "tailwindcss-animate": "^1.0.7",
    "together-ai": "^0.21.1",
    "ws": "^8.18.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-slick": "^0.23.13",
    "@types/ws": "^8.18.0",
    "@vitejs/plugin-react": "^5.1.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9",
    "eslint-config-next": "15.2.1",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "puppeteer": "^24.29.1",
    "tailwindcss": "^4",
    "tsx": "^4.19.3",
    "typescript": "^5",
    "vitest": "^4.0.8"
  }
}
