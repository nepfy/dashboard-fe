{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/_next/(.*)",
      "dest": "/_next/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "wildcard": [
    {
      "domain": "*.nepfy.com",
      "value": "nepfy.com"
    }
  ],
  "env": {
    "NEXT_PUBLIC_ROOT_DOMAIN": "nepfy.com"
  }
}
